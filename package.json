{
  "name": "hax-ai-warp",
  "version": "1.2.0",
  "description": "AI-powered cybersecurity education platform with Docker-based Linux terminals and intelligent tutoring",
  "main": "server.js",
  "bin": {
    "hax-ai-warp": "./bin/hax-ai-warp.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "vm:create": "node scripts/vm-manager.js create",
    "vm:list": "node scripts/vm-manager.js list",
    "vm:cleanup": "node scripts/vm-manager.js cleanup",
    "postinstall": "node scripts/setup.js"
  },
  "keywords": [
    "education",
    "cybersecurity",
    "linux",
    "terminal",
    "docker",
    "ai",
    "tutoring",
    "warp",
    "claude",
    "openai"
  ],
  "author": "HAX AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/djfusco/hax-ai-warp.git"
  },
  "homepage": "https://github.com/djfusco/hax-ai-warp#readme",
  "bugs": {
    "url": "https://github.com/djfusco/hax-ai-warp/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "preferGlobal": false,
  "files": [
    "bin/",
    "lib/",
    "public/",
    "scripts/",
    "server.js",
    "README.md",
    "INSTALL.md",
    "USER-GUIDE.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-pty": "^1.0.0",
    "openai": "^4.104.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
